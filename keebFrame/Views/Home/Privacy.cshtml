
@{
    ViewData["Title"] = "About Us";
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - keebFrame</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
    <script src="~/js/mdb.min.js">hu</script>
    <!-- Bootstrap CSS -->
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.2/css/bootstrap.min.css'>
    <link rel="icon" type="image/x-icon" href="/images/favicon.ico" />
    <style>
        .icon {
            width: 50px;
            height: 50px;
            background-image: url('/images/favicon.ico');
            background-color: white;
            margin: 6px 0;
            background-repeat: no-repeat;
        }

        .divider {
            width: 5px;
            height: auto;
            display: inline-block;
        }

        a {
            color: black;
        }

        .disappear-on-scroll {
            opacity: 1;
        }

        .sticky {
            position: fixed-sticky;
        }

        .white-gradient {
            background: rgb(255,255,255);
            background: linear-gradient(180deg, rgba(255,255,255,0.65) 0%, rgba(255,255,255,0.65) 69%, rgba(0,212,255,0) 100%); 
        }


    </style>

    <script src="https://kit.fontawesome.com/558c0ed736.js" crossorigin="anonymous"></script>

    <!-- https://codepen.io/nickcil/pen/sfutl -->
    <script>
       $(window).scroll(function(){
    $(".disappear-on-scroll").css("opacity", 1 - $(window).scrollTop() / 500);
  });
    </script>

</head>



